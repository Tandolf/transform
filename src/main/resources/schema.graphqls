type Query {
    user: User
    exercises(category: String): [Exercise]!
}

type Mutation {
    createExercise(input: ExerciseInput!): Exercise!
}

type User {
    id: ID!
    givenName: String!
    familyName: String!
    middleName: String!
    email: String!
}

type Exercise {
    id: ID!
    name: String!
}

input ExerciseInput {
    name: String!
}

input CategoriesInput {
    name: String!
}
